{
  "name": "frux-panels",
  "version": "0.0.0",
  "description": "Functional reactive panels UI under React's Flux architecture.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "jsx": "jsx -x jsx src lib",
    "cp": "rsync -av --include='*/' --include='*.js' --exclude='*' src/ lib/",
    "build": "npm run cp & npm run jsx",
    "browserify": "mkdir -p dist/ && browserify index.js -x react -x frux-list -x baconjs -o dist/panels"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/popham/frux-panels.git"
  },
  "keywords": [
    "frp",
    "functional",
    "reactive",
    "react",
    "reactjs",
    "flux",
    "javascript",
    "ui",
    "panels"
  ],
  "author": "Tim Popham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/popham/frux-panels/issues"
  },
  "homepage": "https://github.com/popham/frux-panels",
  "dependencies": {
    "xtend": "^3.0.0"
  }
}
